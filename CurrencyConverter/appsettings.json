{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "CurrencySettings": {
    "ExcludedCurrencies": [ "TRY", "PLN", "THB", "MXN" ]
  },

  "Jwt": {
    "Issuer": "test",
    "Audience": "currency_api",
    "Key": "FlzpFWQLPUKcSECl1kf8lJfry05tjPunVGQEBfZG8YY="

  },
  "OpenTelemetry": {
    "ServiceName": "MyService",
    "Exporter": "Jaeger",
    "Jaeger": {
      "AgentHost": "localhost",
      "AgentPort": 6831
    }
  },
  "CurrencyProviders": {
